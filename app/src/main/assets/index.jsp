<html><head><style>.lineCursor{background-color:green;color:white}body{background-color:rgb(102, 255, 85)}#main{margin:0px 0px;width:80%;}a:link{text-decoration:none;color:blue}a:hover{text-decoration:underline;color:white}</style><title>{TITLE}</title><script>var tdClick=function(td){var childs=td.childNodes;for(var i=0,len=childs.length;i<len;++i){if(childs[i].tagName=="A"){childs[i].click();break;};}};var trs=[];var search=function(input,event){var v=input.value.toLowerCase();for(var i=0,len=trs.length;i<len;++i){if(-1 ==trs[i].childNodes[0].childNodes[1].text.toLowerCase().indexOf(v)){trs[i].style.display="none"}else{trs[i].style.display=""}}};window.onload=function(){trs = document.getElementsByClassName("filetrs");var getTime=function(){return new Date().toLocaleString();};var update=function(){var s=getTime();document.getElementById('now').innerHTML=s;};update();setInterval(update,1000);}</script></head><body><div id='main' style='width:80%;margin:0 auto;'>Current Time: <span style='color:blue' id='now'></span><div style='float:right'><input type='button' value='上传文件' onclick='location.href="/file:///upload.jsp"'/></div><div>{BODY}</div></body></html>